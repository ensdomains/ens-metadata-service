{
  "name": "ens-metadata-service",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Makoto Inoue <2630+makoto@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.170",
    "btoa": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ethers": "^5.3.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "tsc": "^2.0.3",
    "web3": "^1.3.6"
  },
  "scripts": {
    "build": "tsc --project ./",
    "dev": "ENV=local ts-node-dev src/index.ts",
    "start": "node dist/index.js",
    "deploy": "yarn build && ENV=prod gcloud app deploy --project=ens-metadata-service",
    "test": "ava",
    "test:cov": "nyc --reporter=json --reporter=text ava"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.5.11",
    "@types/test-listen": "^1.1.0",
    "ava": "^3.15.0",
    "got": "^11.8.2",
    "nock": "^13.1.1",
    "nyc": "^15.1.0",
    "test-listen": "^1.1.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "test/**/*",
      "!test/entry.mock.ts",
      "!test/interface.ts"
    ],
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
